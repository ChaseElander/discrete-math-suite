  <!-- Modal Trigger -->
  <a class="waves-effect waves-light btn modal-trigger" href="#modal1">Expert</a>

  <!-- Modal Structure -->
  <div id="modal1" class="modal">
    <div class="modal-content">
	  <div class="premise-text">
	  	<textarea placeholder="Enter Premise..." id="premise" rows="4" cols="50"></textarea>
	  </div>
	  <div class="conclusion-text">
	  	<textarea placeholder="Enter Conclusion..." id="conclusion" rows="4" cols="50"></textarea>
	  </div>
	  <div class="proof-text">
	  	<textarea class="proof" placeholder="Enter Proof..." id="proof" rows="4" cols="50"></textarea>
	  </div>
    </div>
    <div class="modal-footer">
      <button onclick="submitText(premise.value, conclusion.value, proof.value)" class="modal-action modal-close waves-effect waves-green btn-flat">Next</a>
    </div>
  </div>

<div class="row">
	<form id="all_proof_input" class="col s12">
		<div id="premises_conclusion_container" class="col s4">
			<div class="row">
				<div id="premise_0" class="input-field col s10 premise_line">
					<input id="input_premise_0" type="text" class="validate premise_line_input">
					<label for="input_premise_0">Premise</label>
				</div>
			</div>
			<div id="premise_secondaries" class="row secondary_line">
				<div class="row">
					<div id="premise_1" class="input-field col s10">
						<input id="input_premise_1" type="text" class="validate premise_line_input">
						<label for="input_premise_1">Premise</label>
					</div>
					<div id="remove_premise_1" class="col s1">
						<a class="btn-floating btn-small waves-effect waves-light red" onclick="deleteRow('remove_premise_1')">
							<i class="material-icons red">cancel</i>
						</a>
					</div>
				</div>
			</div>

			<div id="PremisesAddLine">
				<a class="waves-effect waves-light btn" onclick="addPremiseRow()">
					Add Premise
				</a>
			</div>

			<!-- There's probably a better way to put vertical space -->
			<div class="row"></div>

			<div class="row">
				<div id="conclusion_0" class="input-field col s10 conclusion_line">
					<input id="input_conclusion_0" type="text" class="validate conclusion_line_input">
					<label for="input_conclusion_0">Conclusion</label>
				</div>
			</div>
		</div>



		<div id="proof_container" class="col s7 offset-s1">
			<div class="row">
				<div id="proof_step_0" class="input-field col s5 proof_line">
					<input id="proof_step_input_0" type="text" class="validate proof_line_input">
					<label for="proof_step_input_0">Step</label>
				</div>

				<div id="proof_previous_0" class="input-field col s3 proof_line">
					<input id="proof_previous_input_0" type="text" class="validate proof_line_input">
					<label for="proof_previous_input_0">Previous Lines</label>
				</div>

				<div id="proof_rules_0" class="input-field col s2 proof_line">
					<input id="proof_rules_input_0" type="text" class="validate proof_line_input">
					<label for="proof_rules_input_0">Rules</label>
				</div>
			</div>

			<div id="proof_secondaries" class="row secondary_line">
				<div class="row">
					<div id="proof_step_1" class="input-field col s5">
						<input id="proof_step_input_1" type="text" class="validate proof_line_input">
						<label for="proof_step_input_1">Step</label>
					</div>

					<div id="proof_previous_1" class="input-field col s3 proof_line">
						<input id="proof_previous_input_1" type="text" class="validate proof_line_input">
						<label for="proof_previous_input_1">Previous Lines</label>
					</div>

					<div id="proof_rules_1" class="input-field col s2 proof_line">
						<input id="proof_rules_input_1" type="text" class="validate proof_line_input">
						<label for="proof_rules_input_1">Rules</label>
					</div>

					<div id="proof_remove_1" class="col s1">
						<a id="proof_remove_button_1" class="btn-floating btn-small waves-effect waves-light red" onclick="deleteRow('proof_remove_1')">
							<i class="material-icons red">cancel</i>
						</a>
					</div>
				</div>
			</div>

			<div id="ProofsAddLine">
				<a class="waves-effect waves-light btn" onclick="addProofRow()">
					Add Line
				</a>
			</div>

		</div>

	</form>
</div>
