<% provide(:title, 'All users') %>

<h4>Currently Logged In</h4>
<ul class="users">
    <li>
      <%= link_to current_user.name %>
      <%= link_to current_user.last_seen %>
    </li>
</ul>

<h4>All Users</h4>

<ul class="users">
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name %>
    </li>
  <% end %>
</ul>